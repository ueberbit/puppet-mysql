[client]
host=<%= @host %>
port=<%= @port %>
socket=<%= @socket %>

[server]
datadir=<%= @datadir %>
log-error=<%= "#{@logdir}/error.log" %>
bind-address=<%= @host %>
port=<%= @port %>
socket=<%= @socket %>
max_allowed_packet=128M
max_connections=250
table_open_cache=250
innodb_large_prefix=true
innodb_file_format=barracuda
innodb_file_per_table=true
